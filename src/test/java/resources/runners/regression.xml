<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="regression suits" verbose="1">
    <groups>
        <run>
            <include name="regression"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.saucedemo.customlisteners.CustomListeners"></listener>
    </listeners>
    <test name="regression">
        <classes>
            <class name="com.saucedemo.testsuite.LoginTest"></class>
            <class name="com.saucedemo.testsuite.ProductTest"></class>
        </classes>
    </test>
</suite>